        function langID(ln) {
            return langMap[ln || lang] || 1;
        }
        
        function dolog(v) {
            $('#tlog').html($('#tlog').html() + '<br>' + v);
        }
        
        function langInit() {
            langArr = $.keys(langMap);
            var l = window.location.host.split('.')[0];
            var langID = langMap[l] || 1
            lang = langArr[langID - 1]
            slang = langID == 1 ? 'www' : lang;
            if(l != slang) {
                setLang(slang);
            }
            dolog('lang:' + slang);
        } 
        
        function langInitFoot() {
            var links = $('.lang a');
            links.removeClass('lsel');
            links.each(function(idx, n){
                if($(n).attr('href').indexOf(slang) != -1) {
                    $(n).addClass('lsel');
                }
            });
        }
        
        function setLang(ln) {
            var l = window.location;
            var u = userBase(l.pathname, ln) + l.search + l.hash;
            window.location.href = u;
        }
        
        function masterBase(uri) {
            return 'https://mtr' + this.base + uri;
        }
        
        function userBaseRaw(schema, uri, n) {
            if(n || mainCtx.linfo)
                return schema + (n || mainCtx.linfo.n) + this.base + uri;
            return '';
        }
        
        function userBase(uri, n) {
            return userBaseRaw('https://', uri, n);
        }
        
        function InitBase() {
            var a = window.location.host.split('.');
            a = a.splice(a.length - 2);
            base = '.' + a.join('.');
        }
        
        function LoadCSS(src, after) {
            _get(src, function(data) {
                $('<style rel="stylesheet" type="text/css">' + data + '</style>').appendTo($('head'));
                if(after)
                    after();
            });
        }
        
        function InitScript(src) {
            var data = $('#' + src).text();
            $('<script type="text/javascript">' + data + '<\/script>').appendTo($('head'));
        }
        
        function getScripts(src, after) {
            var r = {i:0};
            for(var i = 0; i < src.length; i++) {
                getScript(src[i], function() {
                    if(++r.i == src.length)
                        after();
                });
            }
        }
        
        function getScript(src, cb) {
            var e, f, h;
            f = true;
            e = document.createElement('script');
            e.type = 'text/javascript';
            e.src = '/' + src;
            h = document.getElementsByTagName('head')[0];
            e.onreadystatechange = e.onload = function() {
                if(f && (!this.readyState || this.readyState == 'complete')) {
                    f = false;
                    cb();
                }
            };
            h.appendChild(e);
        }
        
        function mCall(module, name, args) {
            var m = moduleCtx[module];
            if(args)
                m[name].apply(m, args);
            else
                m[name].call(m);
        }
        
        function fetchI18n(module, base, after) {
            var sn = '_i18n_' + module;
            var s = $('.'+sn);
            var b = base ? base + '/' : ''
            if(s.length == 0) {
                _get('/pub/i18n/' + b  + lang + '/' + moduleBase(module)  + '.js', function(data) {
                    currentModule = module;
                    var e = document.createElement('script');
                    e.type = 'text/javascript';
                    e.text = data;
                    $(e).addClass(sn);
                    $('body').append(e);
                    after();
                }, 'text');
            } else {
                after();
            }
        }
        
        function waitForLast(task) {
            var o = pendingI18n;
            var n = $.Deferred();
            pendingI18n = n;
            var fkt = function() { 
                o.done(function() {
                    if(pendingI18n === n) {
                        var k = $.keys(fixModule);
                        if(k.length == 0) {
                            task(); 
                        } else {
                            if(!_fixLast) {
                                _fixLast = $.Deferred();
                                _fixLast.done(function() {
                                    waitForLast(task);
                                });
                            }
                        }
                    } else {
                        waitForLast(task);
                    }
                    n.resolve();
                }); 
            };
            fkt();
        }
        
        function waitFor(task, ret) {
            var o = pendingI18n;
            var n = $.Deferred();
            pendingI18n = n;
            var fkt = function() { o.done(function() {task(); n.resolve()}); };
            if(ret) {
                return fkt;
            } else {
                fkt();
            }
        }
        
        function addI18n(d) {
            if(++numI18n == 16)
            {
                for(var prop in i18nCtx) {
                    if(!i18nCtx.hasOwnProperty(prop) || prop == 'landing') 
                        continue;
                    var sn = '._i18n_' + prop;
                    $(sn).remove();
                }
                numI18n = 0;
                var landing = i18nCtx['landing'];
                i18nCtx = {landing:landing};
            }
            i18nCtx[currentModule] = d;
        }
        
        function loadI18n(module, i18n, base, after) {
            var next = function() {
                if(after)
                    after();
            }
            if(i18n.length != 0) {
                var m = moduleCtx[module];
                var templ = function() {
                    var p = m ? m.preI18n : null;
                    if(p)
                    {
                        var k =  i18nCtx[module];
                        var n = k || {};
                        p(n, module);
                        if(!k)
                            i18nCtx[module] = n;
                    }
                    $.each(i18n, function(i, n) {
                        var node = $('#'+n)[0];
                        if(node) {
                            var source = node.innerHTML;
                            var tmpl = Hogan.compile(source);
                            var result = tmpl.render(i18nCtx[module]);
                            node.innerHTML = result;
                        }
                    });
                    next();
                };
                var load = !m || !m.noI18nLoad;
                if(load) {
                    fetchI18n(module, base, waitFor(templ, true));
                } else {
                    waitFor(templ);
                }
            } else {
                waitFor(next);
            }
        }
        
        function delStyle(name, module) {
            if(fixModule[module] == undefined) { 
                spf.style.unload(name);
            }
        }
        
        function minit(module, i18n, options, after) {
            dolog(module);
            $.each(i18n, function(i, n) {
                $('#'+n).css('display', 'none');
            });
            
            fixModule[module] = function(oI18n, afterFix) {
                var o = options || empty;
                $.each(o.ideps || empty, function(i, n) {
                    loadI18n(n, i18n, o.i18nBase, function() {
                        
                    });
                });
                loadI18n(module, i18n, o.i18nBase, function() {
                    var mctx = moduleCtx[module] || empty;
                    onLoadComplete(module, oI18n || i18n, mctx.nosel);
                    delete fixModule[module];
                    var refs = o.srefs || empty;
                    var num = {i:refs.length};
                    $.each(refs, function(i, n) {
                        $('<iframe />', {
                            src:'about:blank',
                            style:'display:none',
                            load: function(){
                                this.contentWindow.onunload = function() {
                                    if(--num.i == 0) {
                                        $.each(o.ideps || empty, function(i, n) {
                                            var dctx = moduleCtx[n] || empty;
                                            if(dctx.dep_unload)
                                                dctx.dep_unload();
                                        });
                                        if(mctx.unload)
                                            mctx.unload(); 
                                        delStyle(options.sid, module); 
                                        onUnload(module);
                                    }
                                };
                            }
                        }).appendTo('#'+n);
                    });
                    if(afterFix)
                        afterFix();
                        
                    if(after)
                        after();
                        
                    if(_fixLast) {
                        var fl = _fixLast;
                        _fixLast = null;
                        fl.resolve();
                    }
                });
            }
        }
        
        function path() { 
            var l = window.location.pathname;
            l = l.split('&')[0];
            l = l.substr(1);
            return l;
        }
        
        function onUnload(module) {
            if(fixModule[module] == undefined) { 
                delete moduleCtx[module];           
            }
        }
        
        function onLoadComplete(module, i18n, nosel) {
            dolog('onLoadComplete: ' + module);
            if(!nosel) {
                setActive(module);
            }
            if(moduleCtx[module]) {
                moduleCtx[module].i18n = i18nCtx[module];
                moduleCtx[module]._module = module;
                if(moduleCtx[module].run) {
                    moduleCtx[module].run();
                }
            }
            $.each(i18n, function(i, n) {
                $('#'+n).css('display', '');
            });
        }
        
        function setActive(entry) {
            entry = '/' + entry;
            $('a.sel').removeClass('sel');
            $('a[href^="' + entry + '"]').each(function(idx, e) {
                var href = $(e);
                var uri = href.attr('href').split('?')[0];
                if(uri == entry)
                    href.addClass('sel');
            });
        }
        
        function preInit() {
            if(location.protocol !== 'https:') {
                window.location = location.href.replace('http:', 'https:')
                return;
            }
            if (typeof String.prototype.startsWith != 'function') {
                String.prototype.startsWith = function (str) {
                    return this.slice(0, str.length) == str;
                };
            }
            try {
                getScripts(["pub/js/spf.js", "pub/js/hogan.js", "pub/js/jquery.min.js"], Init);
            } catch(e) {
                dolog(e);
                dolog(e.lineNumber);
            }
        }
        
        function _xhr(type, url, data, after, dataType, xhr) {
            return $.ajax({
              type: type,
              url: url,
              data: data,
              success: after,
              error: function(e) { dolog('ajax fail'); dolog(JSON.stringify(e)); after({r:-1}); },
              dataType: dataType,
              crossDomain: true, 
              xhrFields: xhr
            });
        }
        
        function _get(url, after, dataType) {
            var check = function(v) {
                hideLoad();
                after(v);
            }
            showLoad();
            return _xhr("GET", url, undefined, check, dataType)
        }
        
        function get(url, after, dataType) {
            var check = function(v) {
                hideLoad();
                var data = validateRes(v);
                if(data)
                    after(data);
            }
            showLoad();
            return _xhr("GET", url, undefined, check, dataType)
        }
        
        function _postCredsRaw(url, data, after, dataType) {
            var check = function(v) {
                hideLoad();
                after(v);
            }
            showLoad();
            return _xhr("POST", url, data, check, dataType, { withCredentials: true });
        }
        
        function _postCreds(url, data, after, dataType, nologinCheck) {
            var check = function(v) {
                hideLoad();
                var data = validateRes(v, nologinCheck);
                if(data)
                    after(data);
            }
            showLoad();
            return _xhr("POST", url, data, check, dataType, { withCredentials: true });
        }
        
        function postCreds(url, data, after, dataType) {
            return _postCreds(url, data, after, dataType);
        }
        
        function postCredsCross(din, url, data, after, dataType, _defer) {
            var r = _defer || $.Deferred();
            var check = function(v) {
                hideLoad();
                var res = validateRes(v, true);
                if(res.r != 0 && res.l) {
                    postCredsCross(din, url, data, after, dataType, r); 
                    return;
                }
                r.resolve();
                after(res);
            }
            crossLogin(din.id, function(d) {
                if(d) {
                    din.tk = data.tk = d.tk;
                    showLoad();
                    _xhr("POST", url, data, check, dataType, { withCredentials: true });
                } else {
                    after({r:-1});
                    r.reject();
                }
            }, _defer);
            return r;
        }
        
        function _post(url, data, after, dataType, nologinCheck) {
            var check = function(v) {
                hideLoad();
                var data = validateRes(v, nologinCheck);
                if(data)
                    after(data);
            }
            showLoad();
            return _xhr("POST", url, data, check, dataType);
        }
        
        function post(url, data, after, dataType) {
            return _post(url, data, after, dataType);
        }
        
        function hdrFix() {
            var h = $('.ohdr');
            var m = $('#main');
            var n = $('.mMenu');
            
            if(h.length) {
                m.css('padding-top', h.height());
                n.css('top', h.height() + 1);
            } else {
                m.css('padding-top', '');
                n.css('top', '');
            }
        }
        
        function onResize() {
            var h = $('html');
            h.removeClass('small_m');
            h.removeClass('small');
            var w = $(window);
            var b = $('body');
            var s = w.height();
            var m = $('#menu');
            var hdr = $('.flhdr');
            var sw = h.get(0).scrollWidth;
            var p;
            var sb;
            if(m[0]) {
                var mh = m.height();
                if(mh == 0) {
                    var nm = $(m.children()[0]);
                    if(nm.length) {
                        m = nm;
                        mh = m.height();
                    }
                }
                p = m.offset().top + mh;
                sb = h.innerWidth() != sw;
                if(s < p || sb) {
                    h.addClass('small_m');
                }
                if(sb) {
                    h.addClass('small');
                }
            } 
            placeFrmError();
            hdrFix();
           // hdr.css('padding-right', (sw - bd) + 'px');
        }
        
        function parseSearch(str) {
            if(!str)
                str = window.location.search;
            var a = str.replace(/=/g,'":"').split(/[\?,&]+/);
            if(a[0] == "") {
                a.shift();
            }
            try {
                for (var i in a) {
                    a[i] = decodeURIComponent(a[i]);
                }
                return JSON.parse('{"'+ a.join('","')+'"}');
            } catch(e) {
                return {};
            }
        }
            
        var _mainMenu = {};
        
        function unloadMainMenu() {  
            var o = _mainMenu;
            for(var prop in o) {
                if(!o.hasOwnProperty(prop)) 
                    continue;
                localStorage.removeItem(mainMenuKey(prop));
            }
            clearMainMenu();
        }
        
        function clearMainMenu() {
            _mainMenu = {}; 
        }
            
        function addMainMenu(e) {
            var key = $(e).attr('href');
            _mainMenu[key] = $(e);
        }
        
        function mainMenuKey(n) {
            return "mainMenu_" + n;
        }
        
        function loadMainMenu() {
            var o = _mainMenu;
            for(var prop in o) {
                if(!o.hasOwnProperty(prop)) 
                    continue;
                var r = localStorage.getItem(mainMenuKey(prop));
                if(r) {
                    var e = _mainMenu[prop];
                    e.attr('href', prop + r);
                }
            }
        }
        
        function putSearchMenu(o) { 
            var r = putSearch(o);
            var pn = window.location.pathname;
            var e = _mainMenu[pn];
            e.attr('href', pn + r);
            localStorage.setItem(mainMenuKey(pn), r);
        }
        
        function hreplaceState(a, b, c) {
            if(history.replaceState)
                history.replaceState(a, b, c);
        }
        
        function hpushState(a, b, c) {
            if(history.pushState)
                history.pushState(a, b, c);
        }
        
        function putSearch(o) {
            var first = true;
            var sep = '?';
            var r = "";
            for(var prop in o) {
                if(!o.hasOwnProperty(prop)) 
                    continue;
                r += sep + prop + '=' + o[prop]; 
                if(first) {
                    first = false;
                    sep = '&';
                }
            }
            var out = window.location.protocol + "//" + window.location.host + window.location.pathname + r;
            hpushState({ path: out }, document.title, out);
            return r;
        }
        
        function needLogin(entry, p) {
            return entry.search('_validate') == -1 &&
                   entry.search('password_reset') == -1 &&
                   entry != 'login' &&
                   entry != 'legal' &&
                   entry != 'about' &&
                   entry != 'confirm' &&
                   entry != 'contact' &&
                   !p.startsWith('/doc/');
        }
        
        function fixHreflang() {
            var l = window.location;
            $("[hreflang]").each(function(idx, e) {
                var h = e.href.slice(0, -1);
                e.href = h + l.pathname + l.search + l.hash;
            });
        }
        
        function moduleFromPath(p) {
            var e = p.split('/');
            if(e[e.length-1] == '')
                e.pop();
            if(e[0] == '')
                e.shift();
            return e.join('+');
        }  
        
        function moduleBase(m) {
            var e = m.split('+');
            return e[e.length-1];
        }
            
        function Init() { 
            dolog('next');
            try {
            $.support.cors = true;
            InitBase();
            InitScript('TAB');
            InitScript('JSON');
            langInit();
            fixHreflang(); 
            animFindAnimEvent();
            animFindTransEvent();
            pendingI18n = $.Deferred();
            pendingI18n.resolve();
            LoadCSS('/pub/css/main.css', waitFor(function(){
            }, true));
            LoadCSS('https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,700italic,300,400,700)');
            var entry = path();
            var search  = window.location.search;
            var hash  = window.location.hash;
            $(window).on("spfrequest", function(){
                showLoad();
                $('#content').css('display', 'none');
                spf.style.unload('mstl');
            });
            $(window).on("spfdone", function(e){
                hideLoad();
                var u = e.originalEvent.detail;
                var e = document.createElement('a');
                e.href = u.url;
                u = moduleFromPath(e.pathname);
                if(u == 'doc')
                    u ='doc+doc';
                waitFor(function(){
                    var after = function() {
                        afterLoad();
                        onResize();
                    }
                    if(fixModule[u])
                        fixModule[u](null, after);
                    else
                        after();
                });
            });
            $(window).resize(onResize);
            spf.init({'cache-unified':true});
            var hasEntry = (entry != '');
            var after = function(login) {
                // TODO if we have an err then do not navigate
                var i18n = ['header','footer','menu', 'login'];
                var waiter = null;
                if(hasEntry) {
                    waiter = waitFor(function(){
                        
                    }, true);
                } else {
                    i18n.push('content');
                }
                $('.load').css('display', 'none');
                var e = 'landing' // TODO use entry if login is succesful
                minit(e, ['menu', 'content', 'login', 'theDialog', 'footer'], [], function() {
                    if(hasEntry) {
                        waiter = waitFor(function(){
                            
                        }, true);
                        spf.navigate('/' + entry + search + hash, {onDone:waiter});
                        setActive(entry);
                    }
                });
                fixModule['landing'](i18n);
                
                if(login) {
                    toLogin(hasEntry);
                } else {
                    toLogout();
                }
                if(waiter)
                    waiter();
                waitForLast(afterRender);
            }
                
            tryLogin(hasEntry, entry, after);
            } catch(e) {
                dolog(e);
                dolog(e.lineNumber);
            }
        }
        
        // TODO remove when doc is ready
        function fixDoc() {
            if($('html').hasClass('in_doc')) {
                $('.doc .dinfo').text(i18nCtx['landing'].doc.info);
            }
        }
        
        function afterLoad() {
            fixDoc();
        }
        
        function afterRender() {
            var regFrm = $('#regFrm');
            if(regFrm.length != 0) {
                handleError(regFrm, doRegister, ':', function() {
                    var data = regFrm[0]._sdata || regFrm.serializeJSON();
                    
                    regFrm[0]._sdata = data;
                    if(data.npw !== data.pwr) {
                        var npw = i18nCtx['landing'].reg.pwr;
                        var perr = i18nCtx['landing'].reg.pwm;
                        return [{i:$('#pwr'),l:npw,m:perr}];
                    }
                    return null;
                });
            }
            
            $('.work').css('display', '');
            $('.holder').css('display', '');
            $('.load').css('display', 'none');
            
            goCode(1, true);
            fboxInit();
            iboxInit();
            langInitFoot();
            codeTabInit();
            
            $('#dialogClose').click(function() {
                dialogHide();
            });
            
            setTimeout(function(){
                onResize();
            }, 200);
            
            var l = location ? location : window.location;
            if(l.hash != "") {
                var h = l.hash;
                l.hash = '';
                l.hash = h;
            }
        }
        
        function badLogin() {
            localStorage.removeItem("userMail");
            spf.navigate('/');
        }
        
        function tryLogin(hasEntry, entry, after) {
            var m = localStorage.getItem("userMail");
            var unode = localStorage.getItem('userNode[' + m + ']');
            var last = parseInt(localStorage.getItem('lastLogin'));
            var current = (new Date()).getTime() / 1000;
            var p = window.location.pathname;
            console.log('tryLogin::lastLogin', last, current, current - last, current - last < 3*60*1000);
            var av = false;
            if(!hasEntry || (unode && current - last < 3*60*1000)) {
                var mtr = parseInt(localStorage.getItem("userMaster")) != 0;
                mainCtx.linfo = {m:m,n:unode,mtr:mtr};
                av = unode !== null;
            } else {
                if(needLogin(entry, p)) {
                    badLogin();
                    return;
                }
            }
            
            if(p.startsWith('/doc/')) {
                requireModule('/doc/doc');
            }
            
            after(av);
            
            hreplaceState({success:true}, 'APPUDO', "/");
        }
        
        function loadContent(module, cnt, unload, afterLoad, afterUnload) {
            if(moduleCtx[module])
                return false;
            showLoad();
            res = function() {  
                hideLoad();
                var ul = null;
                if(unload && moduleCtx[module]) {
                    ul = moduleCtx[module][unload] = function(active) {
                        if(cnt.body) {
                            var body = cnt.body;
                            for(var prop in body) {
                                if(!body.hasOwnProperty(prop) || prop == 'minit' || prop == 'misc') 
                                    continue;
                                $('#'+prop).html('');
                            }
                        }
                        if(cnt.head) {
                            var items = $(cnt.head)
                            items.each(function(idx, e) {
                                var name = $(e).attr('name');
                                if(name) {
                                    $('[name=' + name + ']').remove();
                                }
                            });
                        }
                        
                        if(afterUnload)
                            afterUnload(active);
                    }
                }
                fixModule[module](null, function() {
                    if(afterLoad)
                        afterLoad(ul);
                });
            };
            var waiter = null;
            if(!afterLoad) {
                waiter = waitFor(res, true);
            }
            spf.process(cnt, function() {
                if(waiter) {
                    waiter();
                } else {
                    res();
                }
            });
            return true;
        }
        
        function loginPing() {
            if(loginID == -1) {
                loginID = setInterval(function() {
            console.log('tryLogin::lastLogin', (new Date()).getTime() / 1000);
                    localStorage.setItem("lastLogin", (new Date()).getTime() / 1000);
                    postCreds(userBase('/admin/'), {data:JSON.stringify({cmd:15})}, function(res) {
                    });
                }, 3*60*1000);
            }
        }
        
        function toLogin(hasEntry) {
            var _this = this;
            if(moduleCtx['entry_frame'])
                return;
                
            var waiter = waitFor(function() {
                $('body').addClass('in_login');
                fixModule['entry_frame'](null, function() {
                    $(document).attr('title', 'APPUDO Hub');
                    _this.hdrHeight = $('.ohdr').outerHeight(true);
                    updateMaster();
                    
                    if(!hasEntry){
                        spf.navigate('/overview', {onDone:waitFor(function(){
                            fixModule['overview']();
                            onResize();
                            hreplaceState({success:true}, 'APPUDO', '/');
                        }, true)});
                    }
                });
            }, true);
            
            spf.process(logoutFrame, function() {
                waiter();
            });
            
            $('#msg').css('display', 'none');
            $('#loginFrm').css('display', 'none');
            
            var base = userBase('/admin/');
            loginPing();
        }
        
        function toLogout(after) {
            unloadMainMenu();
            spf.style.unload('lout');
            localStorage.removeItem('userMail');
            delete mainCtx.linfo;
            
            var waiter = waitFor(function() {
                $(document).attr('title', 'APPUDO');
                $('body').removeClass('in_login');
                if(fixModule['login_frame'])
                    fixModule['login_frame']();
                delete fixModule['login_frame'];
                updateMaster();
                if(after)
                    after();
                onResize();
            }, true);
            spf.process(loginFrame, function() {
                waiter();
            });
            
            if(loginID != -1) {
                clearInterval(loginID);
                loginID = -1;
            }
        }
        
        function toMain() {
            spf.navigate('/');
        }
        
        function rOk(data) {
            return data.r == 0;
        }
        
        function rEProt(data) {
            return data.r < 0;
        }
        
        function getUserNode(email, after) {
            var n = localStorage.getItem('userNode[' + email + ']');
            if(n) {
                after({r:0,n:n});
            } else {
                _post(masterBase('/admin/'), {data:'{"cmd":3,"data":' + JSON.stringify(email) + '}'}, after, undefined, true);
            }
        }
        
        function doLogin(data, params) {
            console.log('tryLogin::doLogin', (new Date()).getTime() / 1000);
            localStorage.setItem("lastLogin", (new Date()).getTime() / 1000);
            getUserNode(data.email, function(rdata) {
                if(rdata && rdata.r == 0) { 
                    var d = '{"em":' + JSON.stringify(data.email) + ',"pw":' + JSON.stringify(data.password) + '}';
                    _postCreds(userBase('/admin/', rdata.n), {data:'{"cmd":1,"data":' + JSON.stringify(d) + '}'}, function(d) {
                        if(d && d.r == 0) {
                            var linfo = {m:data.email,n:rdata.n,mtr:parseInt(d.m) != 0}; 
                            
                            var setItems = function() {
                                try {
                                    localStorage.setItem('userMail', linfo.m);
                                    localStorage.setItem('userNode[' + linfo.m + ']', linfo.n);
                                    localStorage.setItem('userMaster', d.m);
                                } catch(err) {
                                    localStorage.clear();
                                    return false;
                                }
                                return true;
                            }
                            
                            if(!setItems() && !setItems()) {
                                // TODO local stotage error
                            } else {
                                mainCtx.linfo = linfo;
                                toLogin();
                                hreplaceState({success:true}, 'APPUDO', '/');
                            }
                            
                        } else {
                            spf.navigate('/login'+(params||''));
                        }
                    }, undefined, true);
                } else {
                    spf.navigate('/login'+(params||''));
                }
            });
        }   
        
        function adminRegister(email, invite, after) {
            var d = {n:email,i:invite};
            post(masterBase('/admin/'), {data:'{"cmd":2,"data":' + JSON.stringify(JSON.stringify(d)) + '}'}, after);
        }
        
        function doRegister() {
            var i18n = i18nCtx['landing'].reg;
            var frm = $('#regFrm');
            var data = frm[0]._sdata || frm.serializeJSON();
            var err = function() {
                modalError(i18n.err);
            }
            
            adminRegister(data.nem, data.inv, function(rdata) {
                if(rdata && rdata.r == 0) { 
                    var d = {em:data.nem,pw:data.npw,tk:rdata.t,lID:langID()}
                    post(userBase('/admin/', rdata.n), {data:'{"cmd":0,"data":' + JSON.stringify(JSON.stringify(d)) + '}'}, function(data) {
                        if(data.r == 0) {
                            window.location.href = '/login?reg=1'
                        } else {
                            err();
                        }
                    });
                } else {
                    err();
                }
            });
        }
       
        var pendingLoad = false;
        var loadActive = false;
       
        function showLoad() {
            hasLoad++;
            if(loadActive || pendingLoad)
                return;
            pendingLoad = true;
            setTimeout(function() {
                if(!pendingLoad)
                    return;
                loadActive = true;
                //console.log('showLoad');
                disableMain();
                pendingLoad = false;
                var load = $('#theLoad');
                load.css('display', '');
            }, 50);
        }
        
        function hideLoad() {
            --hasLoad;
            var load = $('#theLoad');
            if(hasLoad == 0) {
                var obj = {};
                obj.hide = function(close) {
                    if(close) {
                        //console.log('hideLoad');
                        if(hasLoad == 0 && loadActive) {
                            loadActive = false;
                            load.css('display', 'none');
                            enableMain();  
                        }
                    } else {
                        setTimeout(function() {obj.hide(true)}, 200);
                    }
                }
                obj.hide();
            }
        }
       
        function validateRes(res, nl) {
            var data = null;
            try {
                data = JSON.parse(res);
                if(!data)
                    return {r:-1};
            } catch(e) {
                return {r:-2};
            }
            if(!nl && data.l) {
                badLogin();
                return {r:-3};
            }
            return data;
        }
        
        function _crossLogin(id, _after, invalid, keep) {
            var after = function(d, end) {
                if(!d) {
                    var cerr = i18nCtx['landing'].dlg.cerr;
                    modalError(cerr);
                }
                _after(d, end);
            }
            if(invalid) {
                delete loginCtx[id];
            }
            
            var getKeep = function() {
                var iid = setInterval(function() {
                    postCreds(userBase('/server/', loginCtx[id].name), {data:'{"cmd":16}',tk:loginCtx[id].tk}, function(res) {
                        
                    });
                }, 2*60*1000);
                return function() {
                    clearInterval(iid);
                }
            }
            
            if(loginCtx[id]) {
                if(keep) {
                    _postCreds(userBase('/server/', loginCtx[id].name), {data:'{"cmd":16}',tk:loginCtx[id].tk}, function(data) {
                        if(data.r == 0) {
                            after(loginCtx[id], getKeep());
                        } else {
                            if(data.l) {
                                _crossLogin(id, _after, true, keep);
                            } else {
                                after({r:-1});
                            }
                        }
                    }, undefined, true);
                } else {
                    after(loginCtx[id]);
                }
                return;
            }
            /* create ticket */
            _postCredsRaw(userBase('/ticket/'), {data:'{"cmd":0,"tn":' + id + '}'}, function(res) {
                var o = undefined;
                var name = null;
                var tk;
                try {
                    r = JSON.parse(res); 
                    if(r.r == 0) {
                        name = r.n;
                        tk = r.tk;
                    }
                } catch(e) {
                    
                }
                
                if(name) {
                    /* add login */
                    _postCredsRaw(userBase('/server/', name), {data:'{"cmd":11}',tk:tk}, function(res) {
                        try {
                            r = JSON.parse(res); 
                            if(r.r == 0) {
                                o = {id:id,tk:r.tk,name:name};
                                loginCtx[id] = o;
                            }
                        } catch(e) {
                        }
                        after(o, keep ? getKeep() : undefined);
                    });
                } else {
                    after(o);
                }
            });
        }
        
        function crossLoginKeep(id, _after, invalid) {
            _crossLogin(id, _after, invalid, true);
        }
        
        function crossLogin(id, _after, invalid) {
            _crossLogin(id, _after, invalid, false);
        }
        
        function crossLogout(id) {
            delete loginCtx[id];
            /* TODO remove login */
        }
        
        function crossLogoutAll() {
            for(var prop in loginCtx) {
                if(!loginCtx.hasOwnProperty(prop)) 
                    continue;
                crossLogout(prop);
            }
        }
        
        function logout() {
            _postCredsRaw(userBase('/admin/'), {data:'{"cmd":2}'}, function(res) {
                var login = $('#theLogin');
                login.css('display', '');
                login.focus();
                toLogout(function() {
                    hreplaceState({success:true}, 'APPUDO', '/');
                    spf.navigate('/logout');
                });
            });
            
            crossLogoutAll();
            unloadMainMenu();
        }
        
        function disableMain() {
            if(modalCount == 0) {
                tabs = [];
                $('body').css('overflow', 'hidden');
                $('#main').find(':tabbable').each(function(idx, e) {
                    tabs.push({idx:e.tabIndex,e:e});
                    e.tabIndex = -1;
                });
            }
            modalCount++;
        }
        
        function enableMain() {
            if(--modalCount == 0) {
                $(tabs).each(function(idx, e) {
                    e.e.tabIndex = e.idx;
                })
                $('body').css('overflow', 'none');
            }
        }
        
        function scroll_to_x(parent, child, after, lmargin) {
            var p = parent[0];
            var br = p.getBoundingClientRect();
            var left = parseInt(br.left);
            var right = parseInt(br.right);
            br = child[0].getBoundingClientRect();
            var cleft = parseInt(br.left) - (lmargin || 0);
            var cright = parseInt(br.right);
        
            var y = 0;
            if(cleft < left) {
                y = -(left - cleft);
            } else 
            if(cright > right) {
                y = cright - right;
            }
            if(y != 0) {
                var v = parent.scrollLeft() + y;
                if(after) {
                    parent.animate( { scrollLeft:v }, 600, 'swing', after);
                } else { 
                    parent.scrollLeft(v);
                }
            } else {
                if(after) 
                    after();
            }
        }
        
        function scroll_to_y(parent, child, after, lmargin) {
            var p = parent[0];
            var br = p.getBoundingClientRect();
            var top = parseInt(br.top);
            var bottom = parseInt(br.bottom);
            br = child[0].getBoundingClientRect();
            var ctop = parseInt(br.top) - (lmargin || 0);
            var cbottom = parseInt(br.bottom);
        
            var y = 0;
            if(ctop >  top) {
                y = -(top - ctop);
            }
            if(y != 0) {
                var v = parent.scrollTop() + y;
                if(after) {
                    parent.animate( { scrollTop:v }, 600, 'swing', after);
                } else { 
                    window. scrollBy(0, v);
                }
            } else {
                if(after) 
                    after();
            }
        }
    
        var _iboxPos;
        var _ibox;
        var _iboxSel = -1;
        var _iboxRdy = true;
        var _iboxInit = false;
        
        function iboxInit() {
            if(!_iboxPos) {
                _iboxPos = [];
                var num = 5;
                _ibox = $('.infoFrm .fbox');
                if(_ibox.length == 0)
                    return;
                for(var i = 0; i < num; i++) {
                    _iboxPos[i] = $('.infoFrm .fbox_item' + i);
                }
                iboxScroll(2, true);
                _iboxInit = true;
            }
        }
        
        function iboxScroll(idx, noanim, first) {
            if(_iboxRdy) {
                _iboxRdy = false;
                if(_iboxSel != -1) {
                    $('.infoFrm .tabsel' + _iboxSel).removeClass('msel');
                } 
                if(_iboxInit)
                {
                    // workaround needed for Safari :(
                    _iboxInit = false;
                    var pl = parseInt(_iboxPos[0].css('margin-left').slice(0, -2));
                    _iboxPos[0].css('margin-left', '0'); 
                    _ibox.scrollLeft(_ibox.scrollLeft() - pl);
                }
                _iboxSel = idx;
                $('.infoFrm .tabsel' + _iboxSel).addClass('msel');
                if(noanim) {
                    scroll_to_x(_ibox, _iboxPos[idx]);
                    _iboxRdy = true;
                } else {
                    scroll_to_x(_ibox, _iboxPos[idx], function() {
                        _iboxRdy = true;
                    });
                }
            }
        }
        
        var _fboxPos;
        var _fbox;
        var _fboxInt = -1;
        var _fboxRdy = true;
        
        function fboxInit() {
            if(!_fboxPos) {
                _fboxPos = [];
                var num = 3;
                _fbox = $('.ldn .fbox');
                if(_fbox.length == 0)
                    return;
                _fbox.scrollLeft(0);
                for(var i = 0; i < num; i++) {
                    _fboxPos.push($('.ldn .fbox_item' + i));
                }
                _fbox.click(function() {
                    fboxScroll((_fbox._theIdx + 1) % num);
                });
                _fbox._theIdx = 0;
                fboxAuto(num);
            }
        }
        
        function fboxScroll(idx, auto) {
            if(_fboxRdy) {
                _fbox._theIdx = idx;
                _fboxRdy = false;
                if(!auto) {
                    clearInterval(_fboxInt);
                    _fboxInt = -1;
                }
                scroll_to_x(_fbox, _fboxPos[idx], function() {
                    _fboxRdy = true;
                }, idx == 0 ? 8 : 0);
            }
        }
        
        function fboxAuto(num) {
            var ctx = {i:0};
            _fboxInt = setInterval(function() {
                ctx.i++;
                var idx = (ctx.i % num);
                fboxScroll(idx, true);
            }, 10000);
        }
        
        function handleError(frm, after, sep, custom) {
            var _frm = frm[0];
    	    var eBox = $('#frmError');
            
            frm.change(function(e) {
                setTimeout(function() {
                    if(eBox[0]._current == frm) {
                        _frm._sdata = null;
                        if(_frm.checkValidity()) {
                            if(custom) {
                                var rr = [];
                    	        rr.s = sep;
                                formError(frm, rr, custom);
                            } else {
                                removeErrors();
                            }
                        }
                    }
                }, 1000);
            });
            
            frm.submit(function(e) {
                e.preventDefault();
                removeErrors();
                var rr = [];
    	        rr.s = sep;
                var er = custom ? formError(frm, rr, custom) : 0;
                if(!er) {
                    _frm._sdata = null;
                    after();
                }
            });
            
            $('<iframe />', {
                src:'about:blank',
                style:'display:none',
                load: function(){
                    this.contentWindow.onunload = function() {
                        if(eBox[0]._current == frm) {
                            removeErrors();
                        }
                    };
                }
            }).appendTo(frm);
            
            _frm.addEventListener('invalid', function(e) {
    	        e.preventDefault();
                if(_frm._ehandled) {
                    _frm._ehandled--;
                    return;
                }
                _frm._ehandled = 0;
    	        var inv = frm.find(':invalid');
                var errs = [];
    	        $.each(inv, function(i, n) {
    	            var input = $('#' + n.id);
    	            var label = $('label[for=' + n.id + ']');
    	            var ltxt = label.length == 0 ? input.attr('placeholder') : label.text();
                    var msg = n.validationMessage || 'Invalid value.';
    	            errs.push({i:input,l:ltxt,m:msg});
    	        });
    	        _frm._ehandled = inv.length - 1;
    	        errs.s = sep;
    	        formError(frm, errs, custom);
            },true);
        }
        
        function placeFrmError(frm) {
    	    var out = $('#frmError');
            var place = out[0]._place;
    	    if(place)
    	        place();
        }
        
        function formError(frm, errs, custom) {   
            var ferr = i18nCtx['landing'].dlg.ferr;
            var abrt = i18nCtx['landing'].dlg.cls;
            var _frm = frm[0];
    	    var out = $('#frmError');
    	    var sep = errs.s || '';
            out.html('');
            
            if(out[0]._current == frm) {
                removeErrors();
            }
            
            if(custom) {
                var c = custom();
                if(c) {
                    errs = errs.concat(c);
                    errs.s = sep;
                }
            }
            if(errs.length) {
            
                var item = $('<h2/>',{'style':'margin-left:1.3em;'}).appendTo(out);
                item.text(ferr);
                
                var list = $('<ol/>').appendTo(out);
                    
                _frm._invalids = [];    
                for(var i = 0; i < errs.length; i++) {
                    var e = errs[i];
                    var input = e.i;
                    item = $('<li/>').appendTo(list);
                    var s = $('<span/>',{'style':'font-weight:bold;margin-right:5px;'}).appendTo(item);
                    s.text(e.l + sep);
                    s = $('<span/>').appendTo(item);
                    s.text( e.m);
                    input.addClass('invalid');
                    _frm._invalids.push(input);
                }
                
                _frm._sdata = null;
            
                var d = $('<div/>',{'style':'text-align:center;'}).appendTo(out);
                var btn = $('<input type="button" value="' + abrt + '"/>',{'style':'float:right;'}).appendTo(d);
                btn.click(function() {
                    removeErrors();
                });
                
        	    out[0]._current = frm;
        	    
        	    var place = function() { 
            	    var o = frm.position();
            	    var l = o.left - (out.outerWidth(true) * 1.05);
                    var w = frm.outerWidth(true);
            	    if(l < 0) {
            	        l += w;
            	    }
            	    
            	    if(errs.length) {
            	        var t = errs[0].i;
                        var o2 = out.position();
                        var left = t.position().left < o.left + (frm.outerWidth(true) / 2);
                        if(o.left < o2.left + out.outerWidth(true) && !left)
                    	    l -= w / 2;
            	    }
            	    
            	    out.css('left', l);
            	    out.css('top', o.top);
            	    out.css('display', 'block');
        	    }
                
        	    out[0]._place = place;
        	    place();
            }
    	    
    	    return errs.length;
        }
        
        function removeErrors() {
    	    var errs = $('#frmError');
    	    var frm = errs[0]._current;
    	    errs.css('display', 'none');
    	    errs[0]._current = null;
    	    errs[0]._place = null;
    	    
    	    if(frm) {
                var _frm = frm[0];
                var inv = _frm._invalids;
                if(inv) {
                    for(var i = 0; i < inv.length; i++) {
                        var e = inv[i];
                        e.removeClass('invalid');
                    }
                }
    	    }
        }
        
        function modalError(msg) {
            var head = i18nCtx['landing'].dlg.err;
            dialogShow(head, msg, 1, function() {
               dialogHide(); 
            });
        }
        
        function modalInfo(msg) {
            var head = i18nCtx['landing'].dlg.inf;
            dialogShow(head, msg, 1, function() {
               dialogHide(); 
            });
        }
        
        function dialogShow(head, msg, buttons, okF, timeout) {
            $('#dialogHead').text(head);
            $('#dialogData').text(msg);
            var close = $('#dialogClose');
            var ok = $('#dialogOk');
            
            ok.off('click');
            ok.on('click', function() {
                if(okF) 
                    okF();
            });
            
            close.css('display', buttons == 1 ? 'none' : 'block');
            
            $('#theDialog').css('display', 'block');
            setModal();
        }
        
        function dialogHide() {
            $('#theDialog').css('display', 'none');
            unsetModal();
        }
        
        function setModal() {
            $('#theBlur').addClass('blur');
            disableMain();
        }
        
        function unsetModal() {
            $('#theBlur').removeClass('blur');
            enableMain();
        }
        
        function updateMaster() {
            var b = $('body');
            if(isMaster()) {
                b.addClass('with_master');
                b.removeClass('no_master');
            } else {
                b.removeClass('with_master');
                b.addClass('no_master');
            }
        }
        
        function isMaster() {
            return mainCtx.linfo && mainCtx.linfo.mtr;
        }
        
        function hasLogin() {
            return mainCtx.linfo;
        }
        
        _nodeByDashID = {}
        
        function getNodeByDashID(did, after) {
            if(_nodeByDashID[did]) {
                after(_nodeByDashID[did]);
            } else {
                postCreds(userBase('/admin/'), {data:JSON.stringify({cmd:31, data:JSON.stringify(did)})}, function(res) {
                    var r = {}
                    if(rOk(res)) {
                        r = {id:res.id,name:res.n};
                        _nodeByDashID[did] = r;
                    }
                    after(r);
                });
            }
        }
        
        function resetNodeByDashID() {
            _nodeByDashID = {}
        }
        
        function clearModule(m) {
            delete moduleCtx[m];
        }
        
        function requireModule(p, after) {
            var m = moduleFromPath(p);
            if(!moduleCtx[m]) {
                var waiter = waitFor(function() { }, true);
                _get(p + _spfNav, function(data) {
                    try {
                        if(!loadContent(m, JSON.parse(data), undefined, after ? after : undefined) && adter)
                            after();
                    } catch(e) {
                        if(after)
                            after();
                    }
                    waiter();
                }, 'text');
            } else {
                if(after)
                    after();
            }
        }
        
