        }
        
        function localStorageRemove(k) {
            
        }
        
        function animFindAnimEvent() {
            var e = document.createElement('elem');
            var items = ['animation', 'OAnimation', 'MozAnimation', 'WebkitAnimation'];
            var res = ['animationend', 'oAnimationEnd', 'animationend', 'webkitAnimationEnd'];
            
            $.each(items, function(i, n) {
                if (e.style[n] !== undefined){
                    _animEventAnim = res[i];
                }
            });
        }
        
        function animFindTransEvent() {
            var e = document.createElement('elem');
            var items = ['transition', 'OTransition', 'MozTransition', 'WebkitTransition'];
            var res = ['transitionend', 'oAnimationEnd', 'transitionend', 'webkitTransitionEnd'];
            
            $.each(items, function(i, n) {
                if (e.style[n] !== undefined){
                    _animEventTrans = res[i];
                }
            });
        }
        
        var _animEventAnim;
        var _animEventTrans;

        function animAddStyle(o, item, name, first) {
            var prefix = ['-o-', '-moz-', '-ms-', '-webkit-', ''];
            var anim = item.anim;
            var txt = '';
            var k;
            
            if(!anim)
                return null;
            
            var step = 100 / anim.length;
            var func = item.func || 'ease-out';
            var time = item.time || 0.001;
            var fill = item.fill || 'forwards';
            var last_origin_x = o.last_origin_x || 0;
            var last_origin_y = o.last_origin_y || 0;
            var last_scale = o.last_scale || 1;
            
            for(k = 0; k < prefix.length; k++) {
                last_origin_x = o.last_origin_x || 0;
                last_origin_y = o.last_origin_y || 0;
                last_scale = o.last_scale || 1;
                txt += '\n@' + prefix[k] + 'keyframes ' + name + '-zoom {';
                for(var j = 1; j < anim.length + 1; j++) {
                    var a = anim[j-1];
                    if(!first) {
                        txt += '0% {';
                        txt += prefix[k] + 'transform-origin:' + last_origin_x + 'px ' + last_origin_y + 'px;';
                        txt += prefix[k] + 'transform:scale(' + last_scale + ');';
                        txt += '}';
                    }
                    txt += (j * step) + '% {';
                    if(a.origin_x != undefined && a.origin_y != undefined) {
                        txt += prefix[k] + 'transform-origin:' + a.origin_x + 'px ' + a.origin_y + 'px;';
                        last_origin_x = a.origin_x;
                        last_origin_y = a.origin_y;
                    }
                    if(a.scale != undefined) {
                        txt += prefix[k] + 'transform:scale(' + a.scale + ');';
                        last_scale = a.scale;
                    }
                    txt += '}';
                }
                txt += '}';
            }
            
            
            o.last_origin_x = last_origin_x;
            o.last_origin_y = last_origin_y;
            o.last_scale = last_scale;
            
            txt += '\n.' + name + ' {'
            
            for(k = 0; k < prefix.length; k++) {
                txt += prefix[k] + 'animation-name:' + name + '-zoom;';
                txt += prefix[k] + 'animation-fill-mode:' + fill + ';';
                txt += prefix[k] + 'animation-timing-function:' + func + ';';
                txt += prefix[k] + 'animation-duration:' + time + 's;';
            }
            
            txt += '}';
            var s = $('<style>');
            s.text(txt);
            s.appendTo('head');
            return s;
        }
        
        function animText(o, frm, item, after) {
            if(item.text) {
                var d = o.last_text || $('<div></div>').insertAfter(frm);
                if(typeof item.text == 'string')
                    d.text(item.text);
                d.addClass('browsertxt');
                d.css('top', item.text_y + 'px');
                d.css('left', item.text_x + 'px');
                delete o.last_text;
                
                var out = function() { 
                    d.removeClass('fadeIn');
                    setTimeout(function() {
                        if(!item.text_noout) {
                            d.addClass('fadeOut');
                            d.one(_animEventAnim, function() {
                                d.remove();
                                after();
                            });
                        } else {
                            o.last_text = d;
                            after();
                        }
                    }, item.text_time || 500);
                }
                
                if(!item.text_noin) {
                    d.addClass('fadeIn');             
                    d.one(_animEventAnim, function() {
                        out();
                    });
                } else {
                    out();
                }
            } else {
                after();
            }
        }
        
        function animClick(ptr, item, after) {
            if(item.clk_x && item.clk_y) {
                ptr.css('top', item.clk_y + 'px');
                ptr.css('left', item.clk_x + 'px');
                ptr.addClass('notrans');
                ptr.removeClass('notrans');
                ptr.addClass('click');
                ptr.one(_animEventTrans, function() {
                    setTimeout(function() {
                        ptr.removeClass('click');
                        after();
                    }, 1);
                });
            } else {
                after();
            }
        }
        
        function animClear(o, frm, num) {
            if(o.styles) {
                if(!num)
                    num = o.styles.length;
                while(num) {
                    num--;
                    var n = o.styles.shift();
                    frm.removeClass(n.name);
                    n.remove();
                }
            }
        }
        
        function animStep(o, frm, ptr, btn, items, idx) {
            var item = items[idx];
            var nm = frm.attr('id') + '_anim_' + idx;
            var st = animAddStyle(o, item, nm, idx == 0);
            idx++;
            var anim = function(e) {
                var after = function() {
                    if(item.bg) {
                        var bg1 = o.bg1;
                        var bg2 = o.bg2;
                        var n = o.bgs[item.bg].clone();
                        if(!item.no_bg_anim) {
                            bg2.replaceWith(n);
                            o.bg2 = bg2 = n;
                            bg1.css('opacity', '0');
                            bg1.one(_animEventTrans, function() {
                                n = o.bgs[item.bg].clone();
                                bg1.replaceWith(n);
                                o.bg1 = bg1 = n;
                                bg1.css('opacity', '1');
                            });
                        } else {
                            bg1.replaceWith(n);
                            o.bg1 = bg1 = n;
                        }
                    }
                    
                    if(idx < items.length) {
                        animStep(o, frm, ptr, btn, items, idx);
                    } else {
                        btn.css('display', 'block');
                        animClear(o, frm);
                        frm.css('background-image', 'url("' + o.bg1.attr('src')  + '")');
                        o.bg1.remove();
                        o.bg2.remove();
                    }
                }
                
                animClick(ptr, item, function() {
                    animText(o, frm, item, after);
                });
            }
            if(st) {
                frm.addClass(nm);
                st.name = nm;
                if(!o.styles)
                    o.styles = [];
                o.styles.push(st);
                if(o.styles.length > 4) {
                    animClear(o, frm, o.styles.length - 4);
                }
                frm.one(_animEventAnim, anim);
            } else {
                anim();
            }
        }
        
        function animPreload(bgs, after) {
            var num = bgs.length;
            var res = {};
            
            if(num != 0)
                showLoad()
            
            for(var i = 0; i < bgs.length; i++) {
                (function (img, src) {
                    var next = function () {  
                        var r = $(img);
                        r.addClass('tabinner');
                        r.addClass('tabbg');
                        res[src] = r;
                        if (--num == 0) {
                            hideLoad();
                            after(res);
                        }
                    };
                    img.onload = next;
                    img.onerror = next;
                    img.onabort = next;
                    img.src = src;
                } (new Image(), bgs[i]));
            }
        }
        
        function animRun(items, frm, ptr, btn) {
            btn.css('display', 'none');
            var o = {};
            var bgs = [];
            for(var i = 0; i < items.length; i++) {
                var bg = items[i].bg;
                if(bg) {
                    bgs.push(bg);
                }
            }
            
            animPreload(bgs, function(res) {
                o.bgs = res;
                
                var bg2 = $('<div></div>').appendTo(frm);
                bg2.addClass('tabinner');
                bg2.addClass('tabbg');
                
                var bg1 = $('<div></div>').appendTo(frm);
                bg1.addClass('tabinner');
                bg1.addClass('tabbg');
                
                o.bg1 = bg1;
                o.bg2 = bg2;
                
                animStep(o, frm, ptr, btn, items, 0);
            });
        }
        
