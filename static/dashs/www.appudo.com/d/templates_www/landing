        function goCode(idx, init) {
            var c = $('.tabcode');
            var m = $('.codemenu');
            var w = $('.cmwhite');
            var a = $('.taba');
            if(c.length != 0) {
                var item = $(c.children()[idx]);
                var mitem = $(m.children()[idx]);
                c.scrollTop(0);
                c.scrollTop(item.position().top);
                a.removeClass('taba');
                a.addClass('tabb');
                mitem.removeClass('tabb');
                mitem.addClass('taba');
                var fix = function() {
                    w.css('display','none');  
                    w.css('left', mitem.position().left + 'px');  
                    w.css('width', (mitem.width() - 5) + 'px');
                    w.css('display','inline-block'); 
                }
                if(init) {
                    setTimeout(function() {
                        fix();
                        setTimeout(function() {
                            fix();
                        }, 500);
                    });
                } else {
                    fix();
                }
            }
        }
        
        function codeTabInit() {
            var tab = $('.tabbox');
            var w = tab.innerWidth();
            $('.tabrun .codeinfo').each(function(idx, n) {
                var a = $(n);
                var s = a.find('span');
                var x = a.position().left;
                s.css('width', (w - x - a.outerWidth(true) - 80) + 'px');
            });
        }
        
        function lightBoxInit(o, items) {
            o.current = 1;
            o.max = items.length;
            o.txt = [];
            var p = $('#' + o.id);
            var r = $('<div>');
            var k = $('<div>');
            var j = $('<div>');
            var c = $('<span>');
            r.appendTo(p);
            k.appendTo(p);
            c.appendTo(k);
            j.appendTo(k);
            r.addClass('row');
            k.addClass('lmodal');
            j.addClass('content');
            var m = $('<div>');
            m.addClass('inner');
            m.appendTo(j);
            j = m;
            c.addClass('close pointer');
            c.html('&times;');
            c.click(function() {
                lightBoxClose(o);
            });
            
            m = $('<div>');
            m.addClass('back');
            m.appendTo(k);
            
            m = $('<div>');
            m.addClass('numbering pointer');
            m.appendTo(k);
            m.click(function() {
                lightBoxList(o);
            });
        
            m = $('<div>');
            m.addClass('prev');
            m.html('&#10094;');
            m.appendTo(k);
            m.click(function() {
                lightBoxNext(o, -1);
            });
            
            m = $('<div>');
            m.addClass('next');
            m.html('&#10095;');
            m.appendTo(k);
            m.click(function() {
                lightBoxNext(o, 1);
            });
            
            $(items).each(function(idx, n) {
                o.txt[idx] = items[idx].t;
                i = $('<div>');
                i.appendTo(r);
                i.addClass('column');
                m = $('<img>');
                m.appendTo(i);
                m.attr('src', n.s);
                m.addClass('pointer');
                m.click(function() {
                    lightBoxOpen(o);
                    lightBoxSet(o, idx + 1);
                });
                
                i = $('<div>');
                i.appendTo(j);
                i.addClass('item');
                m = $('<img>');
                m.appendTo(i);
                m.attr('src', n.s);
                m.click(function() {
                    lightBoxNext(o, 1);
                });
            });  
            
            m = $('<div>');
            m.addClass('txt');
            m.appendTo(k);
        }
        
        function lightBoxList(o) {
            if(!o.list) {
                o.list = 1;
                var p = $('#' + o.id);
                var i = p.find('.inner');
                var t = p.find('.txt');
                var r = p.find('.row');
                var c = p.find('.content');
                r.detach();
                r.appendTo(c);
                i.css('display', 'none');
                t.css('display', 'none');
            }
        }
        
        function lightBoxListClose(o) {
            if(o.list) {
                o.list = 0;
                var p = $('#' + o.id);
                var i = p.find('.inner');
                var t = p.find('.txt');
                var r = p.find('.row');
                r.detach();
                r.appendTo(p);
                i.css('display', 'block');
                t.css('display', 'block');
            }
        }
        
        function lightBoxOpen(o) {
            lightBoxShow(o);
            $('#' + o.id + ' .lmodal').css('display', 'block');
            $('body').css('overflow', 'hidden');
        }
        
        function lightBoxClose(o) {
            $('#' + o.id + ' .lmodal').css('display', 'none');
            $('body').css('overflow', 'auto');
            lightBoxListClose(o);
        }
        
        function lightBoxNext(o, n) {
            o.current += n;
            lightBoxShow(o);
        }
        
        function lightBoxSet(o, n) {
            o.current = n;
            lightBoxShow(o);
        }
        
        function lightBoxShow(o) {
            lightBoxListClose(o);
            var i18n = i18nCtx['landing'].lb;
            var p = $('#' + o.id);
            var items = p.find(".item");
            var sitems = p.find(".small");
            var txt = p.find(".txt");
            var nr = p.find(".numbering");
            var n = o.current;
            o.current = n = n > items.length ? 1 : (n < 1 ? items.length : n);
            items.each(function(idx, n) {
                $(n).css('display', 'none');
            });
            sitems.each(function(idx, n) {
                $(n).removeClass('active');
            });
            
            $(items[n - 1]).css('display', 'block');
            $(sitems[n - 1]).addClass('active');
            txt.html(o.txt[n - 1]);
            nr.html(n + ' / ' + o.max + ' - ' + i18n.ov);
        }
    </script>
</head>
